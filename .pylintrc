# -*-conf-*-
[GENERAL OPTIONS]

[MESSAGES CONTROL]
disable=all
enable=
  syntax-error,
  catching-non-exception,
  consider-using-in,
  inconsistent-return-statements,
  return-in-init,
  trailing-whitespace,
  undefined-variable,
  unidiomatic-typecheck,
  unused-import,
  unused-wildcard-import,
  useless-return,
  super-init-not-called
